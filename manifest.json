{
  "manifest_version": 2,
  "name": "AM custom",
  "version": "0.0.1",
  "background": { "scripts": ["jquery.min.js", "background.js"] },
  "browser_action": {
    "default_icon": "ico.png",
    "default_title": "AM custom",
    "default_popup": "popup/popup.html"
  },
  "permissions" : ["tabs","http://*/*"],
  "content_scripts":[{
    "matches":["http://*/*"],
    "js":["jquery.min.js", "watcher.js"],
    "run_at":"document_end"
  }]
}
