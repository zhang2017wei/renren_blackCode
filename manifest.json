{
  "manifest_version": 2,
  "name": "AM custom",
  "version": "0.0.1",
  "background": {},
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "AM custom",
    "default_popup": "popup/popup.html"
  },
  "permissions" : ["tabs","http://*/*"],
  "content_scripts":[{
